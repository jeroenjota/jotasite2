extends layout.pug
include mixins/tour.pug

block content
  .row
    .col-lg-6.untransp
      each tour in tours
        if tour.soort==="Stad"
          li.mt-2
            +tour(tour, fotos)
    .col-lg-6.untransp
      each tour in tours
        if tour.soort!=="Stad"
          li.mt-2
            +tour(tour, fotos)

